{
    "editor.mouseWheelZoom": true,
    "editor.fontFamily": "'Agave', 'monofur', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontWeight": "normal",
    "workbench.colorTheme": "Bluloco Dark",
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true
    },
    "[rust]": {
        "editor.defaultFormatter": "matklad.rust-analyzer"
    },
    "C_Cpp.updateChannel": "Insiders",
    "local-history.path": "~/Development/vscode-local-history",
    "editor.renderControlCharacters": true,
    "files.autoSave": "onFocusChange",
    "[python]": {
        "gitlens.codeLens.symbolScopes": [
            "!Module"
        ]
    },
    "editor.formatOnSave": true,
    "files.insertFinalNewline": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "terminal.integrated.scrollback": 10000,
    "editor.tabSize": 4,
    "rust-analyzer.updates.channel": "nightly",
    "rust-analyzer.experimental.procAttrMacros": true,
    "rust-analyzer.checkOnSave.allTargets": true,
    "rust-analyzer.checkOnSave.allFeatures": true,
    "rust-analyzer.cargo.allFeatures": true,
    "rust-analyzer.inlayHints.typeHints": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "window.zoomLevel": 1,
    // Inlay hints have been pushing the line length past acceptable.
    // "rust-analyzer.inlayHints.smallerHints": true,
    // "rust-analyzer.inlayHints.chainingHints": false,
}
